{
  "name": "webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix --ignore-pattern 'src/api/*' --ignore-pattern 'src/assets/*' --ignore-pattern 'src/components/ui/*' --ignore-pattern 'src/tests/*'",
    "lint:check": "eslint src --ignore-pattern 'src/api/*' --ignore-pattern 'src/assets/*' --ignore-pattern 'src/components/ui/*' --ignore-pattern 'src/tests/*'",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "generate:openapi-file": "../mvnw -f ../pom.xml --batch-mode package -Popenapi-only && cp ../target/openapi/openapi.json ./docs",
    "generate:api-client": "openapi-ts",
    "generate:api": "npm-run-all generate:openapi-file generate:api-client"
  },
  "dependencies": {
    "@hey-api/openapi-ts": "^0.78.3",
    "@types/node": "^24.0.14",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
